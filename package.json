{
  "name": "@papaya-metaverse/papaya-sdk",
  "version": "0.1",
  "description": "Papaya SDK",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "BUSL-1.1",
  "files": [
    "contracts/*.sol",
    "contracts/helpers",
    "contracts/interfaces",
    "contracts/libraries",
    "contracts/mocks",
    "test/helpers",
    "test/*.js"
  ],
  "dependencies": {
    "@1inch/solidity-utils": "4.2.0",
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "5.0.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "2.0.5",
    "@nomicfoundation/hardhat-ethers": "3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "1.0.10",
    "chai": "4.4.0",
    "dotenv": "16.4.5",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "ethers": "6.11.1",
    "hardhat": "2.22.3",
    "hardhat-dependency-compiler": "1.1.3",
    "rimraf": "5.0.5",
    "solc": "0.8.24",
    "solhint": "4.1.1",
    "ts-node": "10.9.2",
    "typechain": "8.3.2",
    "typedoc": "0.25.12",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "5.4.3"
  },
  "scripts": {
    "clean": "rimraf artifacts cache coverage contracts/hardhat-dependency-compiler artifacts-zk cache-zk",
    "deploy": "hardhat deploy --network",
    "coverage": "hardhat coverage",
    "docify": "npx solidity-utils-docify",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint --max-warnings 0 \"contracts/**/*.sol\" --fix",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:sol:fix",
    "test": "hardhat test --parallel",
    "test:ci": "hardhat test"
  }
